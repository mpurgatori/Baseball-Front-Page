{
  "name": "Baseball Front Page",
  "version": "1.1",
  "description": "An informational landing page for your favorite baseball team.",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "120": "icon120.png",
    "144": "icon144.png"
  },
  "background": {
      "persistent": false,
      "scripts": [ "front_page.js" ]
   },
  "browser_action": {
    "default_title": "Baseball Front Page"
  },
  "permissions": [
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}